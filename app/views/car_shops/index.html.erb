<h1>Your Car Shops</h1>
<div id="car_shops">
  <%= link_to "Create New Car Shop", new_car_shop_path %>
</div>
<table>
  <tr>
    <th>Name</th>
    <th>Category</th>
    <th>Submitted By</th>
  </tr>
  <% @car_shops.each do |shop| %>
    <tr>
      <td><%= shop.name %></td>
      <td><%= shop.category %></td>
      <td><%= shop.user.email %></td>
    </tr>
  <% end %>
</table>
