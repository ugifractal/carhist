<table>
  <thead>
    <tr>
      <th>No</th>
      <th>Email</th>
      <th>Plan</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each_with_index do |user, index| %>
      <tr>
        <td><%= index + 1 %></td>
        <td><%= user.email %></td>
        <td><%= user.plan %></td>
        <td><%= link_to "edit", edit_admin_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
