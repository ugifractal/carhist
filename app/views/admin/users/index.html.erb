<table border="1">
  <thead>
    <tr>
      <th>No</th>
      <th>Email</th>
      <th>Email</th>
      <th>Edit</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each_with_index do |user, index| %>
      <tr>
        <td><%= index + 1 %></td>
        <td><%= user.email %></td>
        <td>
            <%= link_to "free" %>
        </td>
        <td>
            <%= link_to "Edit" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
