<div>
  <%= link_to(admin_car_models_path, class: 'text-red-800') do %>
    &larr; <%= t('views.cars.back') %>
  <% end %>
</div>

<% content_for :title, "New Car" %>

<h1>New Model</h1>
<%= form_with(model: @car_model, url: admin_car_model_path, local: true) do |form| %>
  <%= render "form", car_model: @car_model, form: form %>
  <div class="mt-2">
    <%= form.submit 'Create' %>
  </div>
<% end %>

<br>
