<div class="border border-red-700 mx-5 my-5">
  <div class="flex justify-end px-5 py-5">
    <%= button_to(car_car_maintenance_history_image_path(car, car_maintenance, history), method: :delete, class: "daisy-btn daisy-btn-square", data: { action: "click->upload#delete" }) do %>
      X
    <% end %>
  </div>
  <div>
    <%= link_to(detail_car_car_maintenance_history_image_path(car, car_maintenance, history), class: 'text-red-800') do %>
      <%= image_tag(history.image.url, class: 'px-5 w-60 h-60 object-cover') if history.image %>
    <% end %>
  </div>
</div>
