<div id="<%= dom_id car %>">
  <p>
    <strong>User:</strong>
    <%= car.user_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= car.name %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= car.year %>
  </p>

  <p>
    <strong>Car model:</strong>
    <%= car.car_model_id %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= car.created_at %>
  </p>

  <p>
    <strong>Updated:</strong>
    <%= car.updated %>
  </p>

</div>
