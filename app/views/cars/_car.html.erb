<div id="<%= dom_id car %>">
  <p>
    <strong>Brand:</strong>
    <%= car.brand %>
  </p>

  <p>
    <strong>Model:</strong>
    <%= car.model %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= car.year %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= car.created_at %>
  </p>

  <p>
    <strong>Updated at:</strong>
    <%= car.updated_at %>
  </p>

</div>
