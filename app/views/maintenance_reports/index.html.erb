<div>
  <h1 class="text-2xl"><%= t('.title') %></h1>

  <table class="border-separate border-spacing-y-3">
    <tr>
      <td></td>
    </tr>
    <% @maintenance_reports.each do |report| %>
      <tr>
        <td><%= link_to(format_time(report.created_at, @tz), report.pdf.url, target: '_blank', class: "underline text-red-800") %></td>
      </tr>
    <% end %>
  </table>
  <%= will_paginate(@maintenance_reports) %>
</div>
